<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/startupFragment">

    <fragment
        android:id="@+id/startupFragment"
        android:name="com.example.lockar.StartupFragment"
        android:label="fragment_startup"
        tools:layout="@layout/fragment_startup" >
        <action
            android:id="@+id/action_startupFragment_to_doorDetection"
            app:destination="@id/doorDetection" />
    </fragment>

    <activity
        android:id="@+id/doorDetection"
        android:name="com.example.lockar.DoorDetection"
        android:label="activity_doordetection"
        tools:layout="@layout/activity_door_detection"
        />

</navigation>