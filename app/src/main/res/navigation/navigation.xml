<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/startupFragment">

    <fragment
        android:id="@+id/startupFragment"
        android:name="com.example.lockar.StartupFragment"
        android:label="fragment_startup"
        tools:layout="@layout/fragment_startup" >
        <action
            android:id="@+id/action_startupFragment_to_cameraView"
            app:destination="@id/cameraView" />
    </fragment>
    <fragment
        android:id="@+id/cameraView"
        android:name="com.example.lockar.CameraView"
        android:label="fragment_camera_view"
        tools:layout="@layout/fragment_camera_view" />

</navigation>